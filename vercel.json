{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "headers": [
    {
      "source": "/videos/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Content-Type",
          "value": "video/quicktime"
        }
      ]
    },
    {
      "source": "/(.*).mov",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/quicktime"
        }
      ]
    },
    {
      "source": "/(.*).MOV",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/quicktime"
        }
      ]
    }
  ]
}
